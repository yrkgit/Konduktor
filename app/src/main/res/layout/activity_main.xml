<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backGroundColor"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="201px"
        android:layout_height="67px"
        android:layout_marginStart="21px"
        android:layout_marginTop="45px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_pesa" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="1273px"
        android:layout_height="946px"
        android:layout_marginStart="21px"
        android:layout_marginTop="160px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="27.17px"
        android:layout_height="32.11px"
        android:layout_marginStart="1761.48px"
        android:layout_marginTop="41.22px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_con_color" />
</androidx.constraintlayout.widget.ConstraintLayout>