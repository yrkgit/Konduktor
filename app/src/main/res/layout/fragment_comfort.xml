<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backGroundColor"
    tools:context=".ComfortFragment">

    <!--    OUT_TEMP-->
    <!--    LABEL OUT_TEMP-->

    <TextView
        android:id="@+id/labelOutTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="111dp"
        android:layout_marginTop="23dp"
        android:text="@string/outTemp"
        android:textColor="@color/fontColor"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--    VALUE OUT_TEMP-->

    <TextView
        android:id="@+id/valueOutTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="23dp"
        android:textColor="@color/fontColor"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/labelOutTemp"
        app:layout_constraintTop_toTopOf="parent" />

    <!--    IN_TEMP-->
    <!--    LABEL IN_TEMP-->

    <TextView
        android:id="@+id/labelInTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="111dp"
        android:layout_marginTop="70dp"
        android:text="@string/inTemp"
        android:textColor="@color/fontColor"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--    VALUE IN_TEMP-->

    <TextView
        android:id="@+id/valueInTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="70dp"
        android:textColor="@color/fontColor"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/labelInTemp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/car1"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="172dp"
        android:onClick="onClickButtonTest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/car_design" />
    <ImageView
        android:id="@+id/car2"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginStart="241dp"
        android:layout_marginTop="172dp"
        android:onClick="onClickButtonTest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/car_design" />
    <ImageView
        android:id="@+id/car3"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginStart="371dp"
        android:layout_marginTop="172dp"
        android:onClick="onClickButtonTest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/car_design" />
    <ImageView
        android:id="@+id/car4"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginStart="501dp"
        android:layout_marginTop="172dp"
        android:onClick="onClickButtonTest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/car_design" />


    <!--    COMFORT SECTION-->
    <!--    LINE 1-->

    <View
        android:id="@+id/bottomLine1"
        android:layout_width="214.5dp"
        android:layout_height="4.5dp"
        android:layout_marginStart="10.5dp"
        android:layout_marginTop="515.5dp"
        android:background="@color/selectedButtonColor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <!--    BUTTON COMFORT-->
    <TextView
        android:id="@+id/labelComfort"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:layout_marginTop="482dp"
        android:text="@string/comfort"
        android:textColor="@color/selectedButtonColor"
        android:textSize="20dp"
        android:textStyle="bold"
        android:onClick="onClickComfort"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <!--    SIP SECTION-->
    <!--    LINE 2-->
    <View
        android:id="@+id/bottomLine2"
        android:layout_width="214.5dp"
        android:layout_height="4.5dp"
        android:layout_marginStart="252.5dp"
        android:layout_marginTop="515.5dp"
        android:background="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--    BUTTON SIP-->
    <TextView
        android:id="@+id/labelSip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="255.5dp"
        android:layout_marginTop="482dp"
        android:text="@string/sip"
        android:textColor="@color/fontColor"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <!--    CCTV SECTION-->
    <!--    LINE 3-->
    <View
        android:id="@+id/bottomLine3"
        android:layout_width="214.5dp"
        android:layout_height="4.5dp"
        android:layout_marginStart="495dp"
        android:layout_marginTop="515.5dp"
        android:background="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--    BUTTON CCTV-->
    <TextView
        android:id="@+id/labelCctv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="548.5dp"
        android:layout_marginTop="482dp"
        android:text="@string/cctv"
        android:textColor="@color/fontColor"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <!--    SZP SECTION-->
    <!--    LINE 4-->

    <View
        android:id="@+id/bottomLine4"
        android:layout_width="214.5dp"
        android:layout_height="4.5dp"
        android:layout_marginStart="737dp"
        android:layout_marginTop="515.5dp"
        android:background="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--    BUTTON SZP-->
    <TextView
        android:id="@+id/labelSzp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="760dp"
        android:layout_marginTop="482dp"
        android:text="@string/szp"
        android:textColor="@color/fontColor"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>